{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "ES2020",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"types": ["@cloudflare/workers-types"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./dashboard/src/*"]
		}
	},
	"include": ["./**/*.ts"]
}
